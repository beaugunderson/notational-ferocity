              <Setter Value="{DynamicResource SelectedBackgroundBrush}" TargetName="HoverBorder" Property="Background"></Setter>
              <Setter TargetName="SelectedBorder" Property="Opacity" Value="1"></Setter>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
              <Setter Value="{DynamicResource DisabledForegroundBrush}" Property="Foreground"></Setter>
